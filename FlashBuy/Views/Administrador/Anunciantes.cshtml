@model IEnumerable<FlashBuyClassLibrary.Anunciante>

@{
    ViewBag.Title = "Anunciantes";
    Layout = "~/Views/Shared/_LayoutPageAdministrador.cshtml";
}

<h2>Anunciantes</h2>

<p></p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CPF_CNPJ)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.RazaoSocial)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NomeFantasia)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Telefone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>        
        <th>
            @Html.DisplayNameFor(model => model.VotoPositivo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VotoNegativo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CPF_CNPJ)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RazaoSocial)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NomeFantasia)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Telefone)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>       
        <td>
            @Html.DisplayFor(modelItem => item.VotoPositivo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.VotoNegativo)
        </td>
        @*<td>
            @Html.ActionLink("Edit", "Edit", new { id=item.IdAnunciante }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.IdAnunciante })
        </td>*@
    </tr>
}

</table>
